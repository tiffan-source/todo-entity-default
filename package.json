{
   "name": "todo-entity-default",
   "version": "1.0.0",
   "description": "A simple implementation of a todo entity in TypeScript.",
   "type": "module",
   "repository": {
      "type": "git",
      "url": "https://github.com/tiffan-source/my-ts-startup.git"
   },
   "homepage": "https://github.com/tiffan-source/my-ts-startup.git",
   "scripts": {
      "lint": "npx eslint --fix",
      "format": "npx prettier --write .",
      "format:check": "npx prettier --check .",
      "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
      "test:watch": "npm run test -- --watch",
      "test:coverage": "npm run test -- --coverage",
      "test:debug": "npm run test -- --runInBand --detectOpenHandles --forceExit",
      "prepare": "husky",
      "build": "npx tsc",
      "start": "node dist/src/index.js"
   },
   "keywords": [
      "todo",
      "entity",
      "implementation"
   ],
   "main": "dist/src/index.js",
   "types": "dist/src/index.d.ts",
   "exports": {
       ".": {
           "import": "./dist/src/index.js",
           "types": "./dist/src/index.d.ts"
       }
   },
  "author": "tiffan-source",
   "license": "GNU",
   "devDependencies": {
      "@commitlint/cli": "^19.8.1",
      "@commitlint/config-conventional": "^19.8.1",
      "@eslint/compat": "^1.2.9",
      "@eslint/js": "^9.28.0",
      "@types/jest": "^29.5.14",
      "eslint": "^9.28.0",
      "eslint-config-prettier": "^10.1.5",
      "husky": "^9.1.7",
      "jest": "^29.0.0",
      "lint-staged": "^15.5.2",
      "prettier": "3.5.3",
      "ts-jest": "^29.3.4",
      "ts-node": "^10.9.2",
      "typescript": "^5.8.3",
      "typescript-eslint": "^8.34.0"
   },
   "lint-staged": {
      "**/*.ts": [
         "npm run format",
         "npm run lint"
      ]
   },
   "dependencies": {
      "todo-entity": "1.0.5",
      "uuid": "^11.1.0"
   }
}
